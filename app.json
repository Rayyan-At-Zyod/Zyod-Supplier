{
  "expo": {
    "name": "Zyod Supplier App",
    "slug": "app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/zyod-thumbnail.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/zyod-thumbnail.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.zyod.supplierapp",
      "buildNumber": "1.0.0",
      "requireFullScreen": true,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": ["remote-notification"]
      }
    },
    "android": {
      "windowSoftInputMode": "adjustResize",
      "adaptiveIcon": {
        "foregroundImage": "./assets/zyod-thumbnail.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.zyod.supplierapp",
      "permissions": [
        "ACCESS_NETWORK_STATE",
        "WAKE_LOCK",
        "RECEIVE_BOOT_COMPLETED"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "bccf2b3c-8604-4eff-8272-8b77f2d3bf4f"
      }
    },
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "zyod-jx",
            "project": "zyod-supplier"
          }
        }
      ]
    },
    "plugins": [
      [
        "sentry-expo",
        {
          "organization": "zyod-jx",
          "project": "zyod-supplier"
        }
      ],
      [
        "expo-notifications"
      ],
      [
        "expo-task-manager"
      ],
      [
        "expo-background-fetch"
      ]
    ]
  }
}
